{
  "module": {
    "name": "walkApplication",
    "type": "feature",
    "description": "$string:module_desc",
    "mainElement": "WalkApplicationAbility",
    "deviceTypes": [
      "phone",
      "tablet",
      "2in1"
    ],
    "deliveryWithInstall": true,
    "installationFree": false,
    "pages": "$profile:main_pages",
    "abilities": [
      {
        "name": "WalkApplicationAbility",
        "srcEntry": "./ets/walkapplicationability/WalkApplicationAbility.ets",
        "description": "$string:WalkApplicationAbility_desc",
        "icon": "$media:layered_image",
        "label": "$string:WalkApplicationAbility_label",
        "startWindowIcon": "$media:startIcon",
        "startWindowBackground": "$color:start_window_background",
        "exported": true
      }
    ],
    "extensionAbilities": [
      {
        "name": "WalkApplicationFormAbility",
        "srcEntry": "./ets/walkapplicationformability/WalkApplicationFormAbility.ets",
        "label": "$string:WalkApplicationFormAbility_label",
        "description": "$string:WalkApplicationFormAbility_desc",
        "type": "form",
        "metadata": [
          {
            "name": "ohos.extension.form",
            "resource": "$profile:form_config"
          }
        ]
      }
    ],
    "metadata": [
      {
        "name": "client_id",
        "value": "1589966452859911168"  // 配置为获取的Client ID
      }
    ]
  }
}